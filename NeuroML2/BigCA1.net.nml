    <network id="network_CA1PyramidalCell" type="networkWithTemperature" temperature="35.0 degC">

        <property tag="recommended_dt_ms" value="0.025"/>
        <property tag="recommended_duration_ms" value="10"/>

        <population id="CA1_CG" component="CA1" type="populationList" size="1">
           <annotation>
                <property tag="color" value="0.8984375 0.8984375 0.8984375"/>
            </annotation>
            <instance id="0">
                <location x="0.0" y="0.0" z="0.0"/>
            </instance>
        </population>

        <inputList id="Input_1" component="Input_1" population="CA1_CG">
            <input id="0" target="../CA1_CG/0/CA1" destination="synapses"/>
        </inputList>
    </network>
    <network id="net1">
            <population id="mc1" component="SingleCompMuscleCell" type="populationList">
                <instance id="0">
                    <location x="0" y="0" z="0"/>
                </instance>
                <instance id="1">
                    <location x="10" y="0" z="0"/>
                </instance>
                <instance id="2">
                    <location x="20" y="0" z="0"/>
                </instance>
                <instance id="3">
                    <location x="30" y="0" z="0"/>
                </instance>
                <instance id="4">
                    <location x="40" y="0" z="0"/>
                </instance>
                <instance id="5">
                    <location x="50" y="0" z="0"/>
                </instance>
                <instance id="6">
                    <location x="60" y="0" z="0"/>
                </instance>
            </population>


            <inputList id="offset" component="offsetToMin75" population="mc1">
                <input id="0" target="../mc1/0/SingleCompMuscleCell" destination="synapses"/>
                <input id="1" target="../mc1/1/SingleCompMuscleCell" destination="synapses"/>
                <input id="2" target="../mc1/2/SingleCompMuscleCell" destination="synapses"/>
