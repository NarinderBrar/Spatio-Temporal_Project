    <pulseGenerator id="offsetToMin75" delay="0ms" duration="1000ms" amplitude="-120 pA" />

    <include href="Leak.channel.nml"/>
    <include href="k_slow.channel.nml"/>
    <include href="k_fast.channel.nml"/>
    <include href="ca_boyle.channel.nml"/>
    <include href="SingleCompMuscleCell.cell.nml"/>

    <pulseGenerator id="Input_1" delay="0.002s" duration="0.005s" amplitude="1.8E-9A"/>

    <pulseGenerator id="pulseGen0" delay="5ms" duration="20ms" amplitude="100 pA" />
    <pulseGenerator id="pulseGen1" delay="5ms" duration="20ms" amplitude="200 pA" />
    <pulseGenerator id="pulseGen2" delay="5ms" duration="20ms" amplitude="300 pA" />
    <pulseGenerator id="pulseGen3" delay="5ms" duration="20ms" amplitude="400 pA" />
